module.exports = (sequelize, DataType) => {
  const Adress = sequelize.define('Adress', {
    id: {
      type: DataType.INTEGER,
      primaryKey: true,
      autoIncrement: true
    },
    rua: DataType.STRING(100),
    numero: DataType.STRING(45),
    cep: DataType.STRING(45),
    cidade: DataType.STRING(45),
    estado: DataType.STRING(45),
    
  }, {
    timestamps: false,
    tableName: 'user'
  })
â€‹
  return Adress
}
