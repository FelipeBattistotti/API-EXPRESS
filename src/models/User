module.exports = (sequelize, DataType) => {
  const User = sequelize.define('User', {
    id: {
      type: DataType.INTEGER,
      primaryKey: true,
      autoIncrement: true
    },
    name: DataType.STRING(45),
    email: DataType.STRING(45),
    cpf: DataType.STRING(45),
    cellphone: DataType.STRING(45),
    password: DataType.STRING(100),
    
  }, {
    timestamps: false,
    tableName: 'user'
  })
â€‹
  return User
}